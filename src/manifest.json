{
	"author": "sakuyaa",
	"background": {
		"scripts": ["js/background.js"]
	},
	"browser_action": {
		"browser_style": false,
		"default_icon": "readLater.svg",
		"default_popup": "html/popup.html",
		"default_title": "__MSG_name__"
	},
	"browser_specific_settings": {
		"gecko": {
			"id": "readLater@sakuyaa",
			"strict_min_version": "56.0",
			"update_url": "https://raw.githubusercontent.com/sakuyaa/readLater/master/release/updates.json"
		}
	},
	"default_locale": "en",
	"description": "__MSG_description__",
	"homepage_url": "https://github.com/sakuyaa/readLater",
	"icons": {
		"48": "readLater.svg",
		"96": "readLater.svg"
	},
	"manifest_version": 2,
	"name": "__MSG_name__",
	"options_ui": {
		"browser_style": false,
		"page": "html/options.html"
	},
	"permissions": [
		"<all_urls>",
		"menus",
		"notifications",
		"storage",
		"tabs"
	],
	"version": "3.0"
}